<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:id="@+id/login_layout"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:background="#eeeeee">
	
	<RelativeLayout
		android:id="@+id/title_layout"
		android:layout_width="match_parent"
		android:layout_height="42dp">
		
		<ImageButton
			android:id="@+id/back_btn"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_marginLeft="10dp"
			android:layout_centerVertical="true"
			android:background="@drawable/more_arrow_button_btn"
			android:contentDescription="@string/imgdes"/>
		
		<TextView
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_centerInParent="true"
			android:textSize="18sp"
			android:textColor="#333333"
			android:text="@string/video_more"/>
		
	</RelativeLayout>
	
	<View
		android:id="@+id/title_solid_view"
		android:layout_width="wrap_content"
		android:layout_height="1dp"
		android:layout_below="@id/title_layout"
		android:background="#dedede"/>
	
	<RelativeLayout
		android:id="@+id/head_layout"
		android:layout_width="match_parent"
		android:layout_height="150dp"
		android:layout_below="@id/title_solid_view"
		android:background="#eeeeee">
		
		<ImageView
			android:id="@+id/photo_img"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_centerVertical="true"
			android:layout_marginLeft="48dp"
			android:background="@drawable/more_head_boy_one"
			android:contentDescription="@string/imgdes"
			android:visibility="visible"/>
		
		<ImageView
			android:id="@+id/photo_default_img"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_centerVertical="true"
			android:layout_marginLeft="48dp"
			android:background="@drawable/more_head_bg"
			android:contentDescription="@string/imgdes"/>
		
		<ImageButton
			android:id="@+id/right_btn"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_alignParentRight="true"
			android:layout_marginRight="36dp"
			android:layout_centerVertical="true"
			android:background="@drawable/more_next_button"
			android:contentDescription="@string/imgdes"/>
		
		<TextView
			android:id="@+id/user_name_text"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_marginLeft="17dp"
			android:layout_centerVertical="true"
			android:layout_toRightOf="@id/photo_default_img"
			android:text="@string/username_text"
			android:textColor="#333333"
			android:textSize="16sp"/>
	</RelativeLayout>
	
	<RelativeLayout
		android:id="@+id/local_video_item"
		android:layout_width="match_parent"
		android:layout_height="52dp"
		android:layout_marginTop="26dp"
		android:layout_below="@id/head_layout"
		android:background="#eeeeee">
		
		<ImageView
			android:id="@+id/local_img"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_marginLeft="26dp"
			android:background="@drawable/more_localvideo_icon"
			android:layout_centerVertical="true"
			android:contentDescription="@string/imgdes"/>
		
		<TextView
			android:id="@+id/local_video_text"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_marginLeft="21dp"
			android:layout_toRightOf="@id/local_img"
			android:text="@string/local_video_text"
			android:textSize="14sp"
			android:textColor="#333333"
			android:layout_centerVertical="true"/>
		
		<!--
		<ImageView
			android:id="@+id/share_new_img"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_marginLeft="11dp"
			android:layout_toRightOf="@id/local_video_text"
			android:background="@drawable/local_new"
			android:layout_centerVertical="true"
			android:contentDescription="@string/imgdes"/>
		-->
		
		<ImageView
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_alignParentRight="true"
			android:layout_marginRight="36dp"
			android:background="@drawable/icon_next_press"
			android:layout_centerVertical="true"
			android:contentDescription="@string/imgdes"/>
		
		<View
			android:layout_width="match_parent"
			android:layout_height="1dp"
			android:layout_marginLeft="25dp"
			android:layout_marginRight="25dp"
			android:layout_alignParentBottom="true"
			android:background="#dedede"/>
	</RelativeLayout>
	
	<RelativeLayout
		android:id="@+id/video_square_item"
		android:layout_width="match_parent"
		android:layout_height="52dp"
		android:layout_below="@id/local_video_item">
		
		<ImageView
			android:id="@+id/square_img"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_marginLeft="26dp"
			android:background="@drawable/more_video_icon"
			android:layout_centerVertical="true"
			android:contentDescription="@string/imgdes"/>
		
		<TextView
			android:id="@+id/square_video_text"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_marginLeft="21dp"
			android:layout_toRightOf="@id/square_img"
			android:text="@string/square_title_text"
			android:textSize="14sp"
			android:textColor="#333333"
			android:layout_centerVertical="true"/>
		
		<ImageView
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_alignParentRight="true"
			android:layout_marginRight="36dp"
			android:background="@drawable/icon_next_press"
			android:layout_centerVertical="true"
			android:contentDescription="@string/imgdes"/>
		
		<View
			android:layout_width="match_parent"
			android:layout_height="1dp"
			android:layout_marginLeft="25dp"
			android:layout_marginRight="25dp"
			android:layout_alignParentBottom="true"
			android:background="#dedede"/>
	</RelativeLayout>
	
	<RelativeLayout
		android:id="@+id/draft_item"
		android:layout_width="match_parent"
		android:layout_height="52dp"
		android:layout_below="@id/video_square_item">
		
		<ImageView
			android:id="@+id/draft_img"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_marginLeft="26dp"
			android:background="@drawable/more_draftbox_icon"
			android:layout_centerVertical="true"
			android:contentDescription="@string/imgdes"/>
		
		<TextView
			android:id="@+id/draft_text"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_marginLeft="21dp"
			android:layout_toRightOf="@id/draft_img"
			android:text="@string/draft_box_text"
			android:textSize="14sp"
			android:textColor="#333333"
			android:layout_centerVertical="true"/>
		
		<ImageView
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_alignParentRight="true"
			android:layout_marginRight="36dp"
			android:background="@drawable/icon_next_press"
			android:layout_centerVertical="true"
			android:contentDescription="@string/imgdes"/>
		
		<View
			android:layout_width="match_parent"
			android:layout_height="1dp"
			android:layout_marginLeft="25dp"
			android:layout_marginRight="25dp"
			android:layout_alignParentBottom="true"
			android:background="#dedede"/>
	</RelativeLayout>
	
	<RelativeLayout
		android:id="@+id/shopping_item"
		android:layout_width="match_parent"
		android:layout_height="52dp"
		android:layout_below="@id/draft_item">
		
		<ImageView
			android:id="@+id/shopping_img"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_marginLeft="26dp"
			android:background="@drawable/more_store_icon"
			android:layout_centerVertical="true"
			android:contentDescription="@string/imgdes"/>
		
		<TextView
			android:id="@+id/shopping_text"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_marginLeft="21dp"
			android:layout_toRightOf="@id/shopping_img"
			android:text="@string/shopping_text"
			android:textSize="14sp"
			android:textColor="#333333"
			android:layout_centerVertical="true"/>
		
		<ImageView
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_alignParentRight="true"
			android:layout_marginRight="36dp"
			android:background="@drawable/icon_next_press"
			android:layout_centerVertical="true"
			android:contentDescription="@string/imgdes"/>
		
		<View
			android:layout_width="match_parent"
			android:layout_height="1dp"
			android:layout_marginLeft="25dp"
			android:layout_marginRight="25dp"
			android:layout_alignParentBottom="true"
			android:background="#dedede"/>
	</RelativeLayout>
	
	<RelativeLayout
		android:id="@+id/goluk_item"
		android:layout_width="match_parent"
		android:layout_height="52dp"
		android:layout_below="@id/shopping_item">
		
		<ImageView
			android:id="@+id/goluk_img"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_marginLeft="26dp"
			android:background="@drawable/more_school_icon"
			android:layout_centerVertical="true"
			android:contentDescription="@string/imgdes"/>
		
		<TextView
			android:id="@+id/goluk_text"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_marginLeft="21dp"
			android:layout_toRightOf="@id/goluk_img"
			android:text="@string/goluk_school_text"
			android:textSize="14sp"
			android:textColor="#333333"
			android:layout_centerVertical="true"/>
		
		<ImageView
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_alignParentRight="true"
			android:layout_marginRight="36dp"
			android:background="@drawable/icon_next_press"
			android:layout_centerVertical="true"
			android:contentDescription="@string/imgdes"/>
		
		<View
			android:layout_width="match_parent"
			android:layout_height="1dp"
			android:layout_marginLeft="25dp"
			android:layout_marginRight="25dp"
			android:layout_alignParentBottom="true"
			android:background="#dedede"/>
	</RelativeLayout>
	
	<RelativeLayout
		android:id="@+id/setup_item"
		android:layout_width="match_parent"
		android:layout_height="52dp"
		android:layout_below="@id/goluk_item">
		
		<ImageView
			android:id="@+id/setup_img"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_marginLeft="26dp"
			android:background="@drawable/more_store_icon"
			android:layout_centerVertical="true"
			android:contentDescription="@string/imgdes"/>
		
		<TextView
			android:id="@+id/setup_text"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_marginLeft="21dp"
			android:layout_toRightOf="@id/setup_img"
			android:text="@string/setup_text"
			android:textSize="14sp"
			android:textColor="#333333"
			android:layout_centerVertical="true"/>
		
		<ImageView
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_alignParentRight="true"
			android:layout_marginRight="36dp"
			android:background="@drawable/icon_next_press"
			android:layout_centerVertical="true"
			android:contentDescription="@string/imgdes"/>
		
	</RelativeLayout>
</RelativeLayout>










