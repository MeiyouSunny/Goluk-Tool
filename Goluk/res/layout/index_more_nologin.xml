<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:id="@+id/login_layout"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:background="#eeeeee">
	
	<RelativeLayout
		android:id="@+id/title_layout"
		android:layout_width="match_parent"
		android:layout_height="42dp">
		
	    <RelativeLayout 
	        android:layout_width="50dp"
	        android:layout_height="42dp"
	        android:id="@+id/back_btn_layout"
	        >
	        <ImageButton
			android:id="@+id/back_btn"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_marginLeft="10dp"
			android:layout_centerVertical="true"
			android:background="@drawable/more_arrow_button_btn"
			android:contentDescription="@string/imgdes"/>
	    </RelativeLayout>
		
		<TextView
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_centerInParent="true"
			android:textSize="18sp"
			android:textColor="#333333"
			android:text="@string/video_more"/>
		
	</RelativeLayout>
	
	<View
		android:id="@+id/title_solid_view"
		android:layout_width="wrap_content"
		android:layout_height="1dp"
		android:layout_below="@id/title_layout"
		android:background="#dedede"/>
	
	<RelativeLayout
		android:id="@+id/head_layout_nologin"
		android:layout_width="match_parent"
		android:layout_height="150dp"
		android:layout_below="@id/title_solid_view"
		android:background="#eeeeee">
		
		<ImageView
			android:id="@+id/photo_img_nologin"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_centerVertical="true"
			android:layout_marginLeft="48dp"
			android:background="@drawable/more_head_no_log_in"
			android:contentDescription="@string/imgdes"
			android:visibility="visible"/>
		
		<ImageView
			android:id="@+id/photo_default_img"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_centerVertical="true"
			android:layout_marginLeft="48dp"
			android:background="@drawable/more_head_bg"
			android:contentDescription="@string/imgdes"/>
		
		<ImageButton
			android:id="@+id/right_btn"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_alignParentRight="true"
			android:layout_marginRight="36dp"
			android:layout_centerVertical="true"
			android:background="@drawable/more_next_button"
			android:contentDescription="@string/imgdes"/>
		
		<LinearLayout 
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content"
		    android:orientation="vertical"
		    android:layout_toRightOf="@id/photo_default_img"
		    android:layout_centerVertical="true"
		    >
		    <RelativeLayout 
		        android:layout_width="wrap_content"
		        android:layout_height="wrap_content"
		        >
		         <ImageView 
				    android:id="@+id/user_sex_image_nologin"
				    android:layout_width="wrap_content"
				    android:layout_height="wrap_content"
				    android:layout_marginLeft="15dp"
				    android:layout_centerVertical="true"
				    android:src="@drawable/more_no_log_in_icon"
				    />
		
		<TextView
			android:id="@+id/user_name_text_nologin"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_marginLeft="8dp"
			android:layout_centerVertical="true"
			android:layout_toRightOf="@id/user_sex_image_nologin"
			android:text="@string/user_personal_home_nologin_name"
			android:textColor="#333333"
			android:textSize="16sp"/>
		</RelativeLayout>
		   <TextView 
		       android:layout_width="wrap_content"
		       android:layout_height="wrap_content"
		       android:text="@string/user_personal_home_nologin"
		       android:layout_marginLeft="15dp"
		       android:layout_marginTop="12dp"
		       android:textSize="12sp"
		       android:textColor="@color/user_home_nologin"
		       />
		</LinearLayout>
		
	</RelativeLayout>
	
	<RelativeLayout
		android:id="@+id/local_video_item"
		android:layout_width="match_parent"
		android:layout_height="52dp"
		android:layout_marginTop="26dp"
		android:layout_below="@id/head_layout_nologin"
		android:background="#eeeeee">
		
		<ImageView
			android:id="@+id/local_img"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_marginLeft="26dp"
			android:background="@drawable/more_localvideo_icon"
			android:layout_centerVertical="true"
			android:contentDescription="@string/imgdes"/>
		
		<TextView
			android:id="@+id/local_video_text"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_marginLeft="21dp"
			android:layout_toRightOf="@id/local_img"
			android:text="@string/local_video_text"
			android:textSize="14sp"
			android:textColor="#333333"
			android:layout_centerVertical="true"/>
		
		<!--
		<ImageView
			android:id="@+id/share_new_img"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_marginLeft="11dp"
			android:layout_toRightOf="@id/local_video_text"
			android:background="@drawable/local_new"
			android:layout_centerVertical="true"
			android:contentDescription="@string/imgdes"/>
		-->
		
		<ImageView
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_alignParentRight="true"
			android:layout_marginRight="36dp"
			android:background="@drawable/icon_next_press"
			android:layout_centerVertical="true"
			android:contentDescription="@string/imgdes"/>
		
		<View
			android:layout_width="match_parent"
			android:layout_height="1dp"
			android:layout_marginLeft="25dp"
			android:layout_marginRight="25dp"
			android:layout_alignParentBottom="true"
			android:background="#dedede"/>
	</RelativeLayout>
	
