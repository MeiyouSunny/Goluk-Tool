<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="ExtImage">
        <attr name="extSrc" format="reference"></attr>
        <attr name="extBorderLineWidth" format="dimension"></attr>
        <attr name="extBorderRoundRadius" format="dimension"></attr>
    </declare-styleable>
    <declare-styleable name="VerSeekbar">
        <attr name="thumb" format="reference"></attr>
        <attr name="VerProgressDrawable" format="reference"></attr>
    </declare-styleable>
    <declare-styleable name="extAutoTitle">
        <attr name="sText" format="string"></attr>
    </declare-styleable>
    <declare-styleable name="extdragarray">
        <attr name="sArrays" format="reference"></attr>
    </declare-styleable>
    <declare-styleable name="extColorPicker">
        <attr name="isDrawCircle" format="boolean"></attr>
        <attr name="isDrawStrokeOnly" format="boolean"></attr>
        <attr name="isTextEdit" format="boolean"></attr>
    </declare-styleable>
    <declare-styleable name="ExtCircleProgressBar">
        <attr name="extprogressDrawable" format="reference"/>
        <attr name="extindetermiateDrawable" format="reference"/>
        <attr name="extindetermiate" format="boolean"/>
    </declare-styleable>
    <declare-styleable name="RangeSeekBar">
        <attr name="isSide" format="boolean"></attr>
    </declare-styleable>

    <!-- 字幕颜色显示是否描边 -->
    <declare-styleable name="squareview">
        <attr name="square_text" format="string"></attr>
        <attr name="square_bg" format="color"></attr>
    </declare-styleable>
    <declare-styleable name="SwitchButton">
        <attr name="swThumbDrawable" format="reference"/>
        <attr name="swThumbColor" format="color|reference"/>
        <attr name="swThumbMargin" format="dimension|reference"/>
        <attr name="swThumbMarginTop" format="dimension|reference"/>
        <attr name="swThumbMarginBottom" format="dimension|reference"/>
        <attr name="swThumbMarginLeft" format="dimension|reference"/>
        <attr name="swThumbMarginRight" format="dimension|reference"/>
        <attr name="swThumbWidth" format="dimension|reference"/>
        <attr name="swThumbHeight" format="dimension|reference"/>
        <attr name="swThumbRadius" format="dimension|reference"/>
        <attr name="swBackRadius" format="dimension|reference"/>
        <attr name="swBackDrawable" format="reference"/>
        <attr name="swBackColor" format="color|reference"/>
        <attr name="swFadeBack" format="boolean"/>
        <attr name="swBackMeasureRatio" format="float"/>
        <attr name="swAnimationDuration" format="integer"/>
        <attr name="swTintColor" format="color|reference"/>
        <attr name="swTextOn" format="string"/>
        <attr name="swTextOff" format="string"/>
        <attr name="swTextMarginH" format="dimension"/>
        <attr name="swAutoAdjustTextPosition" format="boolean"/>
    </declare-styleable>
    <!-- 圆形带圆角选择 支持进度-->
    <declare-styleable name="ExtCircle">
        <attr name="circleChecked" format="boolean"/>
        <attr name="circleBgColor" format="color|reference"/>
        <attr name="circleBorderColor" format="color|reference"/>
        <attr name="circleBorderWidth" format="dimension"/>
    </declare-styleable>
    <!-- 矩形带圆角选择-->
    <declare-styleable name="ExtSquare">
        <attr name="squareChecked" format="boolean"/>
        <attr name="squareBgColor" format="color|reference"/>
        <attr name="squareBorderColorP" format="color|reference"/>
        <attr name="squareBorderColorN" format="color|reference"/>
        <attr name="squareBorderWidth" format="dimension"/>
        <attr name="squareCornersRadius" format="dimension"/>
    </declare-styleable>

    <declare-styleable name="PuzzleView">
        <attr name="line_size" format="integer"/>
        <attr name="line_color" format="color"/>
        <attr name="selected_line_color" format="color"/>
        <attr name="handle_bar_color" format="color"/>
        <attr name="need_draw_line" format="boolean"/>
        <attr name="need_draw_outer_line" format="boolean"/>
        <attr name="piece_padding" format="dimension"/>
        <attr name="animation_duration" format="integer"/>
        <attr name="radian" format="float"/>
    </declare-styleable>



    <declare-styleable name="CircleImageView">
        <attr name="border_width" format="dimension" />
        <attr name="border_color" format="color" />
    </declare-styleable>
    <declare-styleable name="EffectRadioButton">
        <attr name="rbUncheckedAlpha" format="float" />
    </declare-styleable>
    <declare-styleable name="CircleAnimationView">
        <attr name="loadingSrc" format="reference"/>
        <attr name="uncheckedAlpha" format="float" />
    </declare-styleable>

</resources>